#include <stdio.h>
#include <string.h>

int main()
{
    int flag=0,i;
    char s1[]="Ravish";
    char s2[]="Acharya";
    
    for(i=0;s1[i]!='\0'||s2[i]!='\0';i++)
    {
        if(s1[i]!=s2[i])
        {
            flag=1;
            break;
        }
    }
    if(flag==1)
    {
        printf("String is not same");

    }
    else
    printf("same");
    
    return 0;
}
